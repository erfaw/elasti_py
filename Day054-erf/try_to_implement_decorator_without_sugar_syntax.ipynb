    "import time\n",
    "def delay_decorator(function):\n",
    "    def wrapped_f():\n",
    "        print(\"w8 for 2s...\")\n",
    "        time.sleep(2)\n",
    "        function()\n",
    "    return wrapped_f\n",
    "say_hello = delay_decorator(say_hello)"
   ]
